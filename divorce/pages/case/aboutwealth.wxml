<!--pages/case/aboutwealth.wxml-->
<view class='line_h40 font_14 color_6 bg_f2 p_lr_20'>请根据你的个人情况，回答如下问题：</view>
<form bindsubmit='formSubmit'>
  <view class='formbox bg_f'>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>车辆：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
        <view>辆</view>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>房产：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
        <view>套</view>
      </view>
    </view>
    <view class="p_lr_36 section">
      <picker mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
        <view class="picker line_h40 font_14 color_3 arrow_right">
          房产是否银行按揭： {{time}}
        </view>
      </picker>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>有限公司：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section">
      <picker mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
        <view class="picker line_h40 font_14 color_3 arrow_right">
          公司是否有外人参股： {{time}}
        </view>
      </picker>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>独资企业：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>股票：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>基金：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>银行存款：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>知识产权：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
    <view class="p_lr_36 section line_h40 font_14 color_3">
      <view class='border_b_dc flex'>
        <view>手机号：</view>
        <input type='text' placeholder='请输入' class='flex_1'></input>
      </view>
    </view>
  </view>
  <view class="section mt_50 mb_50 p_lr_36">
    <button formType="submit" class="bg_theme color_f">提交</button>
  </view>
</form>