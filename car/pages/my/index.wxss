/* pages/my/index.wxss */
page{
  background-color: #f3f4f6;
}
.header_box{
  height: 330rpx;
}
.header{
  width: 180rpx;
  height: 180rpx;
  border: 2px solid #ffffff;
  border-radius: 50%;
  box-sizing: border-box;
}
.list{
  
}
.item{
  float: left;
  width: 50%;
  padding: 15rpx;
  box-sizing: border-box;
  position: relative;
}
.item_title{
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.item_img{
  display: block;
  width: 100%;
  height: 246rpx;
}