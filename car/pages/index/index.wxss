/* pages/index/index.wxss */
page{
  background-color: #f3f4f6;
}
.index_logo{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.shopname{
  line-height: 80rpx;
}
.btn_buy{
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #f9295d;
  margin: 16rpx 0;
}
.icon_position{
  width: 16px;
  height: 18px;
}
.icon_tel{
  width: 25px;
  height: 25px;
}
.shop_model_icon{
  width: 106rpx;
  height: 106rpx;
}
.list{
  
}
.item{
  float: left;
  width: 50%;
  padding: 15rpx;
  box-sizing: border-box;
  position: relative;
}
.item_title{
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.item_img{
  display: block;
  width: 100%;
  height: 246rpx;
}